    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
    <script>
        let audio = document.querySelector(".music-play");
        let ply = 0;
        document.querySelectorAll(".play-btn").forEach(elem=>{
            elem.addEventListener("click",()=>{
                if(ply == 0){
                    document.querySelectorAll(".ply-btn").forEach(pl=>{
                        pl.style.display = "none"
                    })
                    document.querySelectorAll(".pau-btn").forEach(py=>{
                        py.style.display = "inline"
                    })
                    document.querySelector(".ply-now").textContent = "Pause now"
                    audio.play()
                    ply = 1;
                }else{
                    document.querySelectorAll(".ply-btn").forEach(pl=>{
                        pl.style.display = "inline"
                    })
                    document.querySelectorAll(".pau-btn").forEach(py=>{
                        py.style.display = "none"
                    })
                    document.querySelector(".ply-now").textContent = "Play now"
                    audio.pause()
                    audio.load()
                    ply = 0;
                }
            })
        })

        document.addEventListener("DOMContentLoaded", ()=>{
            audio.addEventListener('timeupdate', () => {
            const currentTime = audio.currentTime;
                const duration = audio.duration;
                const progress = (currentTime / duration) * 100;
                document.querySelector(".music-progress").style.width = `${progress}%`;

                const currentMinutes = Math.floor(currentTime / 60);
                const currentSeconds = Math.floor(currentTime % 60);
                const durationMinutes = Math.floor(duration / 60);
                const durationSeconds = Math.floor(duration % 60);

                const formattedCurrentTime = `${currentMinutes}:${currentSeconds < 10 ? '0' : ''}${currentSeconds}`;
                const formattedDuration = `${durationMinutes}:${durationSeconds < 10 ? '0' : ''}${durationSeconds}`;
            if(ply == 1){
                document.querySelector(".duration").textContent = `${formattedCurrentTime} / ${formattedDuration}`;
                document.querySelectorAll(".audio-duration").forEach(elem=>{
                    elem.textContent = `${formattedDuration}`;
                })
            }
        });
        })

        document.querySelectorAll(".card-pg").forEach(elem=>{
            const myslide =  elem.querySelector(".card-pg .mySwiper")
            const mynext =  elem.querySelector(".card-pg .swiper-button-next")
            const mypre =  elem.querySelector(".card-pg .swiper-button-prev")
            var swiper = new Swiper(myslide, {
            slidesPerView: 6.5,
            // centeredSlides: true,
            spaceBetween: 20,
            loop:true,
            grabCursor: true,
            // mousewheel: true,
            keyboard: true,
            navigation: {
                nextEl: mynext,
                prevEl: mypre,
            },
        });
        })

        let menu = 0;
        document.querySelector(".menu-btn").addEventListener("click",()=>{
            if(menu == 0){
                document.querySelector(".menu").style.display = "flex"
                menu = 1;
            }else{
                document.querySelector(".menu").style.display = "none"
                menu = 0
            }
        })
    </script>
