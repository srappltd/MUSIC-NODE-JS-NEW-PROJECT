<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Free Music Online - Deshboard</title>
    <link rel="shortcut icon" href="https://www.wynk.in/assets/icons/icon-384x384.png" type="image/x-icon">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"/>
    <link rel="stylesheet" href="../stylesheet/style.css">
</head>
<body>
    <div id="main">
        <%- include("user-nav.ejs")%>
        <!-- // slider top  -->
        <% if(allSliders.length != 0){%>
        <div class="slider-page">
            <!-- Swiper -->
            <div class="swiper mySwiper">
                <div class="swiper-wrapper">
                        <%allSliders.reverse().forEach(slider=>{%>
                            
                            <div class="swiper-slide"><a href="#" class="slider-img"><img src="../admin/slider/<%= slider.picture%>" alt=""></a></div>
                        <%})%>
                </div>
                <div class="swiper-button-next">
                    <span class="w-6 h-6 text-white-right"><svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true"><path stroke-linecap="round" stroke-linejoin="round" d="M9 5l7 7-7 7"></path></svg></span>
                </div>
                <div class="swiper-button-prev">
                    <span class="w-6 h-6 text-white-left"><svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true" ><path stroke-linecap="round" stroke-linejoin="round" d="M15 19l-7-7 7-7"></path></svg></span>
                </div>
            <!-- <div class="swiper-pagination"></div> -->
            </div>
        </div>
        <%}%>

        <% if(allCategory.length != 0){%>
            <% allCategory.reverse().forEach(category=>{%>
                <!-- // <%= category.title%> Release  -->
                <% if(category.songid.length != 0){%>
                    <div class="card-pg">
                        <h2><%= category.title%></h2>
                        <div class="card-pg-btm">
                            <!-- Swiper -->
                            <div class="swiper mySwiper">
                                <div class="swiper-wrapper">
                                    
                                        <% category.songid.reverse().forEach(songs=>{%>
                                            <div class="swiper-slide">
                                                <a href="/play?v=<%= songs._id%>" class="card-a">
                                                    <div class="card">
                                                        <img src="../uploads/<%= songs.poster%>" alt="">
                                                        <div class="card-cover">
                                                            <span>
                                                                <svg class="bi bi-play-fill" fill="currentColor" height="24" viewBox="0 0 16 16" width="24" xmlns="http://www.w3.org/2000/svg"><path d="m11.596 8.697-6.363 3.692c-.54.313-1.233-.066-1.233-.697V4.308c0-.63.692-1.01 1.233-.696l6.363 3.692a.802.802 0 0 1 0 1.393z"/></svg>
                                                            </span>
                                                        </div>
                                                    </div>
                                                    <p class="title"><%= songs.title%></p>
                                                </a>
                                            </div>
    
                                        <%})%>
                                </div>
                                <div class="swiper-button-next">
                                    <span class="w-6 h-6 text-white-right"><svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true"><path stroke-linecap="round" stroke-linejoin="round" d="M9 5l7 7-7 7"></path></svg></span>
                                </div>
                                <div class="swiper-button-prev">
                                    <span class="w-6 h-6 text-white-left"><svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true" ><path stroke-linecap="round" stroke-linejoin="round" d="M15 19l-7-7 7-7"></path></svg></span>
                                </div>
                                <div class="swiper-pagination"></div>
                            </div>
                        </div>
                    </div>
                <%}%>
            <%})%>
        <%}%>

        <% if(allCategory.length == 0){%>
            <% allCategory.forEach(category=>{%>
                <!-- // Top Indie Artists  -->
                <% if(category.artistid[0].types == "artist"){%>
                    <div class="card-pg">
                        <h2><%= allCategory.title%></h2>
                        <div class="card-pg-btm">
                            <!-- Swiper -->
                            <div class="swiper mySwiper">
                                <div class="swiper-wrapper">
                                    <% if(category.songid.length != 0){%>
                                        <% category.songid.reverse().forEach(songs=>{%>
                                            <div class="swiper-slide">
                                                <a href="#" class="card-a">
                                                    <div class="card-circle">
                                                        <img src="https://img.wynk.in/unsafe/220x220/filters:no_upscale():strip_exif():format(webp)/http://s3.ap-south-1.amazonaws.com/discovery-prod-arsenal/arsenal/artworks/656d965729754375e3025530/COLLECTION_106362634348246.png" alt="">
                                                        <div class="card-cover">
                                                            <span>
                                                                <svg class="bi bi-play-fill" fill="currentColor" height="24" viewBox="0 0 16 16" width="24" xmlns="http://www.w3.org/2000/svg"><path d="m11.596 8.697-6.363 3.692c-.54.313-1.233-.066-1.233-.697V4.308c0-.63.692-1.01 1.233-.696l6.363 3.692a.802.802 0 0 1 0 1.393z"/></svg>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </a>
                                            </div>
                                        <%})%>
                                    <%}%>
                                    
                                    
                                    
                                </div>
                                <div class="swiper-button-next">
                                    <span class="w-6 h-6 text-white-right"><svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true"><path stroke-linecap="round" stroke-linejoin="round" d="M9 5l7 7-7 7"></path></svg></span>
                                </div>
                                <div class="swiper-button-prev">
                                    <span class="w-6 h-6 text-white-left"><svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" aria-hidden="true" ><path stroke-linecap="round" stroke-linejoin="round" d="M15 19l-7-7 7-7"></path></svg></span>
                                </div>
                                <div class="swiper-pagination"></div>
                            </div>
    
                            
                        </div>
                    </div>
                <%}%>
                
            <%})%>
        <%}%>
        
        

        <!-- // footer  -->
        <footer>
            <div class="footer-lt">
                <a href="#">ABOUT US</a>
                <a href="#">PRIVACY POLICY</a>
                <a href="#">TERMS OF USE</a>
                <a href="#">CONTACT US</a>
                <a href="#">HELLOTUNES</a>
            </div>
