    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
    <script>
        var swiper = new Swiper(".slider-page .mySwiper", {
            slidesPerView: "auto",
            spaceBetween: 20,
            // cssMode: true,
            // mousewheel: true,
            keyboard: true,
            loop:true,
            navigation: {
                nextEl: ".slider-page .swiper-button-next",
                prevEl: ".slider-page .swiper-button-prev",
            },
        });

        document.querySelectorAll(".card-pg").forEach(elem=>{
            const myslide =  elem.querySelector(".card-pg .mySwiper")
            const mynext =  elem.querySelector(".card-pg .swiper-button-next")
            const mypre =  elem.querySelector(".card-pg .swiper-button-prev")
            var swiper = new Swiper(myslide, {
            slidesPerView: 6.5,
            // centeredSlides: true,
            spaceBetween: 20,
            loop:true,
            grabCursor: true,
            // mousewheel: true,
            keyboard: true,
            navigation: {
                nextEl: mynext,
                prevEl: mypre,
            },
        });
        })
        
    </script>
    <script>
        async function SliderData(){
            let slideData = await axios.get("http://localhost:3000/admin/slider/")
            slideData.data.forEach(data=>{
                console.log(data.picture)
            })
        }
        SliderData()
        
    </script>
